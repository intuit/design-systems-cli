{
  "name": "@design-systems/cli",
  "version": "1.4.4",
  "description": "A cli for scaffolding and building design systems",
  "author": "Adam Dierkens <adam@dierkens.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "start": "yarn build --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "ds": "./bin/ds.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intuit/design-systems-cli.git"
  },
  "dependencies": {
    "@design-systems/cli-utils": "link:../cli-utils",
    "@design-systems/core": "link:../core",
    "@design-systems/load-config": "link:../load-config",
    "command-line-application": "0.9.6",
    "deepmerge": "4.2.2",
    "dlv": "1.1.3",
    "dotenv": "8.2.0",
    "env-ci": "5.0.1",
    "fs-extra": "8.1.0",
    "tslib": "1.10.0",
    "update-check": "1.5.3"
  },
  "keywords": [
    "cli"
  ],
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/env-ci": "3.1.0"
  }
